<?xml version="1.0"?>

<project name="Pet Clinic Swing RCP" default="usage" basedir=".">
    <property file="build.properties"/>
    <property file="${common.build.properties}"/>
	<import file="${common.build.xml}"/>
 
    <!-- Construct compile classpath -->
    <path id="project.compile.classpath">
		<fileset dir="${spring.dist.dir}">
			<include name="spring.jar"/>
            <include name="spring-sandbox.jar"/>
		</fileset>
    	<pathelement path="${spring-rcp.resources.dir}"/>
    	<fileset dir="${spring-rcp.dist.dir}">
   			<include name="spring-rcp.jar"/>
    	</fileset>
		<fileset dir="${spring-rcp.lib.dir}">
			<include name="**/*.jar"/>
		</fileset>
    	<fileset dir="${spring.lib.dir}">
    		<include name="**/*.jar"/>
   		</fileset>
    	<pathelement path="${petclinic.build.dir}"/>
    </path>

	<target name="copy-dist-dependencies">

	</target>

</project>
